def busqueda_lineal(lista, valor):
    posicion = 0
    encontrado = False

    while posicion < len(lista) and not encontrado:
        if lista[posicion] == valor:
            encontrado = True
        else:
            posicion +=1

        return encontrado, posicion

numeros = [10 , 30 , 35 , 44 , 67 , 88]
